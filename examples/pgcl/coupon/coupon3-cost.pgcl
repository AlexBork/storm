function coupon3() {
	int coup0 := 0;
	int coup1 := 0;
	int coup2 := 0;

	int draw1 := 0;
	int draw2 := 0;
	int draw3 := 0;

	int cost := 1;

	while (!(coup0 = 1) | !(coup1 = 1) | !(coup2 = 1)) {
		draw1 := unif(0,2);
		draw2 := unif(0,2);
		draw3 := unif(0,2);
		cost := ceil(1.02 * cost);

		if(draw1 = 0 | draw2 = 0 | draw3 = 0) {
