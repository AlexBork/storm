mdp

module one
    x : [0..4];
    y : [0..2];
    
    [] (x=0) -> (x'=1) & (y'=1);
    [] (x=1) -> (x'=1) & (y'=1);
    [] (x=0) -> (x'=2) & (y'=1);
    [] (x=2) -> (x'=4) & (y'=2);
    [] (x=0) -> (x'=3) & (y'=2);
    [] (x=3) -> (x'=3) & (y'=2);
    [] (x=4) -> (x'=2) & (y'=1);
    
endmodule
    
    
